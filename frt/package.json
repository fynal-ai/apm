	"scripts": {
		"dev": "vite dev --host 0.0.0.0 --port 13173",
		"inspect": "node --inspect -e \"const { createServer } = require('vite'); (async () => { const server = await createServer({ server: { host: '0.0.0.0', port: 13173 } }); server.listen(); })()\"",
